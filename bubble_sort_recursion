#include<bits/stdc++.h>
using namespace std;

    void BubbleSort(vector<int>&arr, int n ){
        
    if( n==1 ){
        return ;
    }
    for(int i=0; i<n-1; i++){
        if(arr[i] > arr[i+1]){
            swap( arr[i] , arr[i+1] );
        }
    }
    BubbleSort( arr, n-1 );
    
    }

int main(){
    
    vector<int>arr = {6,4,7,9,1,0,2,55,6,33};
    int n = 10;
    BubbleSort( arr, n );
    cout<<"sorted array :"<<endl;
    for(int i= 0; i<n; i++){
        cout<<arr[i]<<" ";
    }
    cout<<endl;
    return 0;
    
} 
